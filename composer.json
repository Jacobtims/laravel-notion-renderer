{
    "name": "rehankanak/laravel-notion-renderer",
    "description": "Laravel Package for Converting Notion Pages to Web Pages",
    "keywords": [
        "rehankanak",
        "laravel-renderer",
        "notion",
        "notion-renderer",
        "notion-laravel-renderer"
    ],
    "homepage": "https://github.com/zusamarehan/laravel-notion-renderer",
    "license": "MIT",
    "authors": [
        {
            "name": "KU Rehan",
            "email": "zrehan286@gmail.com",
            "homepage": "https://github.com/zusamarehan/laravel-notion-renderer",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.6",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "saloonphp/pagination-plugin": "^2.2",
        "saloonphp/saloon": "^3.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RehanKanak\\LaravelNotionRenderer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RehanKanak\\LaravelNotionRenderer\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RehanKanak\\LaravelNotionRenderer\\NotionRendererServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": []
}
